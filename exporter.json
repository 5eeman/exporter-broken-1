{
    "id": "io.supernova.documentation-main",
    "name": "Documentation",
    "description": "Default Supernova documentation exporter",
    "author": "Jiri Trecak",
    "organization": "Supernova",
    "source_dir": "src",
    "assets_dir": "assets",
    "version": "1.5.3",
    "config": {
        "sources": "sources.json",
        "output": "output.json",
        "js": "src/js_helpers.js"
    },
    "engines": {
        "pulsar": "1.0.0",
        "supernova": "1.0.0"
    },
    "type": "documentation",
    "tags": [
        "Documentation"
    ],
    "contributes": {
        "configuration": [
            {
                "key": "defaultVisualMode",
                "default": "dark",
                "type": "enum",
                "values": [
                    "light",
                    "dark"
                ],
                "label": "Default design theme",
                "description": "Base theme configuration for the entire design system documentation. Can be additionally overriden with separate values.",
                "category": "Design"
            },
            {
                "key": "showVisualModeSwitch",
                "default": true,
                "type": "boolean",
                "label": "Dark/Light mode switch",
                "description": "Allow users to switch from light to dark documentation using menu item",
                "category": "Design"
            },
            {
                "key": "googleAnalyticsAPIKey",
                "default": "",
                "type": "string",
                "label": "Google Analytics API key",
                "description": "Add Google Analytics tracking to your documentation site by providing an API key",
                "category": "Analytics"
            },
            {
                "key": "intercomAPIKey",
                "default": "",
                "type": "string",
                "label": "Intercom API key",
                "description": "Add Intercom to your documentation site by providing an API key",
                "category": "Analytics"
            }
        ],
        "blocks": []
    }
}
