{
    "id": "io.supernova.documentation-main",
    "name": "Documentation",
    "description": "Default Supernova documentation exporter",
    "author": "Jiri Trecak",
    "organization": "Supernova",
    "source_dir": "src",
    "assets_dir": "assets",
    "version": "1.0.0",
    "config": {
        "sources": "sources.json",
        "output": "output.json",
        "js": "src/js_helpers.js"
    },
    "engines": {
        "pulsar": "1.0.0",
        "supernova": "1.0.0"
    },
    "tags": [
        "Documentation"
    ],
    "contributes": {
        "configuration": [
            {
                "key": "social_twitter",
                "default": "twitter.com",
                "label": "Social Links - Twitter",
                "type": "string"
            },
            {
                "key": "social_facebook",
                "default": "facebook.com",
                "label": "Social Links - Facebook",
                "type": "string"
            },
            {
                "key": "social_instagram",
                "default": "instagram.com",
                "label": "Social Links - Instagram",
                "type": "string"
            },
            {
                "key": "social_github",
                "default": "github.com",
                "label": "Social Links - GitHub",
                "type": "string"
            },
            {
                "key": "social_dribbble",
                "default": "dribbble.com",
                "label": "Social Links - dribbble",
                "type": "string"
            },
            {
                "key": "show_footer",
                "default": true,
                "label": "Show footer",
                "type": "boolean"
            },
            {
                "key": "show_header",
                "default": true,
                "label": "Show header",
                "type": "boolean"
            },
            {
                "key": "footer_copyright",
                "default": "Copyright Â© ACME Corporations  2021",
                "label": "Footer Copyright Label",
                "type": "string"
            },
            {
                "key": "dark_mode",
                "default": false,
                "label": "Use dark mode",
                "type": "boolean"
            },
            {
                "key": "fullscreen_mode",
                "default": false,
                "label": "When used, documentation will expand to full width of the page. Otherwise, it will be constrained to 1200px box",
                "type": "boolean"
            }
        ]
    }
}
